        6) echo "Development audit feature coming soon..." ;;
        7) echo "System optimization feature coming soon..." ;;
        8) nexus_nova_help ;;
        [Qq]*) echo "Exiting..."; return 0 ;;
        *) echo "Invalid option"; sleep 2; nexus_nova_dashboard ;;
    esac
