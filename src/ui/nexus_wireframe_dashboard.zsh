# ============================================================================
# INTERACTIVE DASHBOARD ENGINE
# ============================================================================

nexus_enhanced_dashboard() {
  local current_view="main"
  local split_mode=false
  
  while true; do
    case "$current_view" in
      main)
        nexus_draw_main_dashboard
        ;;
      chat)
        nexus_draw_chat_panel
        ;;
      command)
        nexus_draw_command_panel
        ;;
      code)
        nexus_draw_code_panel
        ;;
      visual)
        nexus_draw_visual_panel
        ;;
      split)
        nexus_draw_split_view
        ;;
    esac
    
    echo ""
    echo -e "${NOVA_COLORS[cyan]}Navigation:${NOVA_COLORS[reset]}"
    echo "  [F6] Chat | [F7] Command | [F8] Code | [F9] Visual | [F10] Split View"
    echo "  [M] Main Menu | [H] Help | [Q] Quit"
    echo ""
    echo -n "Select: "
    
    read -k 1 choice
    echo ""
    
    case "$choice" in
      1|m|M) current_view="main" ;;
      6) current_view="chat" ;;
      7) current_view="command" ;;
      8) current_view="code" ;;
      9) current_view="visual" ;;
      0) current_view="split" ;;
      h|H) nexus_show_wireframe_help ;;
      q|Q) break ;;
      *)
        # Route to appropriate dashboard based on number
        case "$choice" in
          1) command -v nova_ai_dashboard &>/dev/null && nova_ai_dashboard ;;
          2) command -v nova_iot_dashboard &>/dev/null && nova_iot_dashboard ;;
          3) command -v nova_crypto_dashboard &>/dev/null && nova_crypto_dashboard ;;
          4) command -v nova_automation_dashboard &>/dev/null && nova_automation_dashboard ;;
          5) command -v nova_monitor_dashboard &>/dev/null && nova_monitor_dashboard ;;
          6) current_view="visual" ;;
        esac
        ;;
    esac
  done
}

